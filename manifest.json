{
  "manifest_version": "1",
  "type": "WebApp",
  "meta": {
    "name": "Valorant Game Events Consumer - HubGamers",
    "version": "1.0.0",
    "minimum-overwolf-version": "0.220.0.1",
    "author": "AlexisBRIET",
    "icon": "assets/icon.png",
    "icon_gray": "assets/icon_gray.png",
    "launcher_icon": "assets/desktop_icon.ico",
    "description": "Game Events Consumer Sample"
  },
  "data": {
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [
        21640
      ]
    },
    "start_window": "index",
    "windows": {
      "index": {
        "open_dev_tools": true,
        "file": "index.html",
        "focus_game_takeover": "ReleaseOnHidden",
        "focus_game_takeover_release_hotkey": "ges_showhide",
        "is_background_page": false,
        "transparent": false,
        "resizable": true,
        "show_in_taskbar": true,
        "size": {
          "width": 1150,
          "height": 535
        },
        "start_position": {
          "Top": 10,
          "Left": 10
        }
      }
    },
    "game_events": [
      21640
    ],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [
            21640
          ]
        },
        "start_minimized": false
      }
    ]
  }
}